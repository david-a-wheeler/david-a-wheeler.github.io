000000r 1               SSIZE     =128           ; sector size in bytes
000000r 1               NBUF      =8             ; number of buffers desired in RAM
000000r 1               ;                             (SSIZE*NBUF >= 1024 bytes)
000000r 1               SECTR     =800           ; sector per drive
000000r 1               ;                              forcing high drive to zero
000000r 1               SECTL     =1600          ; sector limit for two drives
000000r 1               ;                              of 800 per drive.
000000r 1               BMAG      =1056          ; total buffer magnitude, in bytes
000000r 1               ;                              expressed by SSIZE+4*NBUF
000000r 1               ;
000000r 1               BOS       =$20           ; bottom of data stack, in zero-page.
000000r 1               TOS       =$9E           ; top of data stack, in zero-page.
000000r 1               N         =TOS+8         ; scratch workspace.
000000r 1               IP        =N+8           ; interpretive pointer.
000000r 1               W         =IP+3          ; code field pointer.
000000r 1               UP        =W+2           ; user area pointer.
000000r 1               XSAVE     =UP+2          ; temporary for X register.
000000r 1               ;
000000r 1               TIBX      =$0100         ; terminal input buffer of 84 bytes.
000000r 1               ORIG      =$0300         ; origin of FORTH's Dictionary.
000000r 1               MEM       =$4000         ; top of assigned memory+1 byte.
000000r 1               UAREA     =MEM-128       ; 128 bytes of user area
000000r 1               DAREA     =UAREA-BMAG    ; disk buffer space.
