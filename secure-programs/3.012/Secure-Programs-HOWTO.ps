1.432 354.103 M(    \([A-Za-z0-9\\-\\_\\.\\!\\~\\*\\'\\\(\\\)]|\(%[2-9A-Fa-f][0-9a-fA-F]\)\)+\)?)S
1.432 343.303 M(  \(\(/\([A-Za-z0-9\\-\\_\\.\\!\\~\\*\\'\\\(\\\)]|\(%[2-9A-Fa-f][0-9a-fA-F]\)\)+\)*/?\) # path)S
1.432 332.503 M(   \(\\#\([A-Za-z0-9\\-\\_\\.\\!\\~\\*\\'\\\(\\\)\\+]|\(%[2-9A-Fa-f][0-9a-fA-F]\)\)+\)? # fragment)S
1.432 321.703 M(  \)|)S
