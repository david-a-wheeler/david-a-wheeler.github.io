1.432 103.303 M(        \(\\&\([A-Za-z0-9\\-\\_\\.\\!\\~\\*\\'\\\(\\\)\\+]|\(%[2-9A-Fa-f][0-9a-fA-F]\)\)+=)S
1.432 92.503 M(         \([A-Za-z0-9\\-\\_\\.\\!\\~\\*\\'\\\(\\\)\\+]|\(%[2-9A-Fa-f][0-9a-fA-F]\)\)+\)*\))S
